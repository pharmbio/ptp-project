	Mesh = {Databases, Factual; Discriminant Analysis; Genomics; Least-Squares Analysis; Linear Models; Molecular Targeted Therapy; Neural Networks (Computer); Pharmaceutical Preparations; Pharmacological Phenomena; Protein Binding},
	Number = {5},
