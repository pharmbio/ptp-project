	Mesh = {Computer Simulation; Models, Chemical; Molecular Conformation; Quantitative Structure-Activity Relationship; Regression Analysis},
	Month = {Jun},
	Number = {6},
	Pages = {1596-603},
	Pmid = {24797111},
	Pst = {ppublish},
	Title = {Introducing conformal prediction in predictive modeling. A transparent and flexible alternative to applicability domain determination},
	Volume = {54},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/ci5001168}}

